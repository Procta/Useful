# vim
alias vi='vim'

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

#Git alias's
alias gs='git status'
alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gco='git checkout'
alias gd='git diff'
alias gk='gitk --all&'
alias gm='git merge'
alias gx='gitx --all'
alias gr='git reset'

#cd Variations
alias ..="cd .."
alias ..2="cd ../.."
alias ..3="cd ../../.."
alias ..4="cd ../../../.."
alias ..5="cd ../../../../.."

#staffingnation vpn
alias vpn_qa='sudo openvpn ~/openvpn/qa.ovpn'
alias vpn_prod='sudo openvpn ~/openvpn/prod_vpn.ovpn'

#staffingnation environments
alias qa='ssh ec2-user@vpn_qa -i ~/keys/qa-final.pem'
alias prod='ssh ec2-user@vpn_prod -i ~/keys/production.pem'
