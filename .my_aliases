#Copy this file to the home directory 
#Add "source ~/.my_aliases" to .bashrc and .zshrc

# vim
alias vi='vim'

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

#Git alias's
alias gs='git status'
alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gco='git checkout'
alias gd='git diff'
alias gk='gitk --all&'
alias gm='git merge'
alias gx='gitx --all'
alias gr='git reset'

#cd Variations
alias ..="cd .."
alias ..2="cd ../.."
alias ..3="cd ../../.."
alias ..4="cd ../../../.."
alias ..5="cd ../../../../.."
